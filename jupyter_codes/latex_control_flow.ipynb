   "source": [
    "import latexify\n",
    "\n",
    "@latexify.algorithmic\n",
    "def control_flow(x):\n",
    "    n = 0\n",
    "    while x > 1:\n",
    "        n = n + 1\n",
    "        if x % 2 == 0:\n",
    "            x = x // 2\n",
    "        else:\n",
    "            x = 3 * x + 1\n",
    "    return n\n",
    "\n",
    "control_flow"
   ]

   "source": [
    "import sympy as sp\n",
    "from IPython.display import display, Math\n",
    "\n",
    "x = sp.Symbol(\"X\")\n",
    "\n",
    "equation = sp.Eq(sp.sin(x) ** 2 + sp.cos(x) ** 2, 1)\n",
    "\n",
    "latex_equation = sp.latex(equation)\n",
    "\n",
    "display(Math(latex_equation))"
   ]

    "food_item = input(\"Enter Food Item Name: \")\n",
    "\n",
    "food_items_dict = {\n",
    "    \"Burger\": 100,\n",
    "    \"Pizza\": 200,\n",
    "    \"Juice\": 300,\n",
    "    \"Tofu\": 400,\n",
    "}\n",
    "\n",
    "def get_price(food_item):\n",
    "    return food_items_dict.get(food_item, 0)\n",
    "\n",
    "print(get_price(food_item))"
